{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.26.3",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all format lint test clean && tsc",
    "start": "PORT=80 node build/src",
    "local": "ts-node src",
    "local:watch": "nodemon src -e ts,json --exec 'npm run local'",
    "lint": "eslint src",
    "format": "pnpm prettier --write src",
    "format:check": "pnpm prettier --check src",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "prettier",
    "jest",
    "supertest"
  ],
  "author": "Richard Ryan",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@pinata/sdk": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "1.4.5-lts.1"
  }
}
